{
  "name": "corinth.js",
  "version": "0.2.0",
  "description": "Node.js wrapper for Corinth, a Rust message queue server.",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --fix --ext .ts",
    "test": "ava test/**/*.spec.ts -c 1",
    "coverage": "nyc npm test",
    "postcoverage": "nyc report --reporter=json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/marvin-j97/corinth.js.git"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "message queue",
    "wrapper",
    "api"
  ],
  "author": "marvin-j97",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/marvin-j97/corinth.js/issues"
  },
  "homepage": "https://github.com/marvin-j97/corinth.js",
  "devDependencies": {
    "@types/debug": "^4.1.5",
    "@types/node": "^14.11.10",
    "@types/semver": "^7.3.4",
    "@typescript-eslint/eslint-plugin": "^4.4.1",
    "@typescript-eslint/parser": "^4.4.1",
    "ava": "3.15.0",
    "concurrently": "^6.0.2",
    "eslint": "^7.11.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-simple-import-sort": "^5.0.3",
    "nyc": "^15.1.0",
    "semver": "^7.3.2",
    "ts-node": "^9.0.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.0.3"
  },
  "ava": {
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register",
      "tsconfig-paths/register"
    ]
  },
  "dependencies": {
    "debug": "^4.2.0",
    "haxan": "^0.2.0"
  }
}
