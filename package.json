{
  "name": "typescript-eslint-ava-template",
  "version": "1.0.0",
  "description": "Typescript Eslint Ava Boilerplate",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --fix --ext .ts",
    "test": "ava test/**/*.spec.ts -c 1",
    "coverage": "nyc npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dotvirus/typescript-eslint-ava-template.git"
  },
  "keywords": [],
  "author": "dotvirus",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/dotvirus/typescript-eslint-ava-template/issues"
  },
  "homepage": "https://github.com/dotvirus/typescript-eslint-ava-template#readme",
  "devDependencies": {
    "@types/debug": "^4.1.5",
    "@types/node": "^14.11.10",
    "@types/semver": "^7.3.4",
    "@typescript-eslint/eslint-plugin": "^4.4.1",
    "@typescript-eslint/parser": "^4.4.1",
    "ava": "3.12.1",
    "concurrently": "^5.3.0",
    "eslint": "^7.11.0",
    "eslint-config-prettier": "^6.13.0",
    "eslint-plugin-simple-import-sort": "^5.0.3",
    "nyc": "^15.1.0",
    "semver": "^7.3.2",
    "ts-node": "^9.0.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.0.3"
  },
  "ava": {
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register",
      "tsconfig-paths/register"
    ]
  },
  "dependencies": {
    "debug": "^4.2.0",
    "haxan": "^0.0.1"
  }
}
